import java.util.*;
public class Main
{
	public static void main(String[] args) {
	Scanner Sc = new Scanner(System.in);
	String s=Sc.nextLine();
	int decimal=0;
	for (int i = s.length()-1; i>=0; i--) {
            char ch = s.charAt( i);
             if (ch == '1') {
                decimal += Math.pow(2, s.length() - 1 - i);
            }
        }
        System.out.println(decimal);
	}
}
