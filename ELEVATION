public class Main {
    public static void main(String[] args) {
        int Height = 30; 
        int upperElevation = 10; 
        int lowerElevation = 5; 
        int count =0;
        int sum=0;
        for(int j=1;j<Height;j++){
            if(sum!=Height){
                if(j%2!=0){
                    sum=sum+upperElevation;
                    count++;
                }
                else{
                    sum=sum-lowerElevation;
                    count++;
                }
            }
        }

        System.out.println("Total time to reach " + Height + " meters is: " + count + " seconds");
    }
}
