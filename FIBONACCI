import java.util.*;
public class Main
{
    public static int fib(int n){
    if(n==0){
        return 0;
    }
    else if(n==1){
        return 1;
    }
    return fib(n-1)+fib(n-2);
}
	public static void main(String[] args) {
	Scanner SC = new Scanner(System.in);
	int n=SC.nextInt();
	 int res=fib(n);
	 System.out.println(res);
	}
	
}
