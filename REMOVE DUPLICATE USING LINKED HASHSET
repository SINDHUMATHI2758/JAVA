import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    String name="Sindhumathi";
		Set<Character>seen=new LinkedHashSet<>();
		StringBuilder res=new StringBuilder();
		for(char ch:name.toCharArray()){
		    seen.add(ch);
		}
		for(char ch:seen){
		    res.append(ch);
		}
 

        System.out.println(res.toString());
    }
}
