import java.util.*;
public class Main
{

    
	public static void main(String[] args) {
		String s="aabccccdddaa";
		String s2="";
		int count=1;
		for(int i=0;i<s.length();i++){
		    if(i+1<s.length()&&s.charAt(i)==s.charAt(i+1)){
		        count++;
		    }
		    else{
		    s2=s2+s.charAt(i);
		    s2=s2+count;
		    count=1;
		        
		    }
		    
		}
		System.out.println(s2);
	}
}
